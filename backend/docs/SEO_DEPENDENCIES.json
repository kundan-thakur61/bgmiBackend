{
  "seo_dependencies": {
    "backend": {
      "helmet": "^7.1.0",
      "compression": "^1.7.4",
      "express-rate-limit": "^7.1.5"
    },
    "frontend": {
      "web-vitals": "^3.5.0"
    }
  },
  "installation_commands": {
    "backend": "cd backend && npm install helmet compression express-rate-limit",
    "frontend": "cd frontend && npm install web-vitals"
